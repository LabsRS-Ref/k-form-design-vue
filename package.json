{
  "name": "k-form-design",
  "version": "3.7.91",
  "private": false,
  "description": "基于vue、ant-design-vue的表单设计器,可视化开发表单",
  "license": "MIT",
  "author": "kchengz",
  "keywords": [
    "ant-design",
    "form",
    "k-form-design",
    "表单设计器",
    "表单设计",
    "可视化",
    "工作流"
  ],
  "main": "lib/k-form-design.umd.min.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib": "vue-cli-service build --target lib --name k-form-design --dest lib packages/index.js && npm run mini",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue packages/",
    "mini": "vue-cli-service build --target lib --name k-form-design-mini --dest lib packages/mini.js --no-clean",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@floating-ui/dom": "^0.4.2",
    "ant-design-vue": "^1.7.6",
    "clipboard": "^2.0.6",
    "clone-deep": "^4.0.1",
    "moment": "^2.24.0",
    "mutationobserver-shim": "^0.3.7",
    "serialize-javascript": "^6.0.0",
    "vcolorpicker": "^1.1.0",
    "vue": "^2.6.10",
    "vue-codemirror-lite": "^1.0.4",
    "vue-draggable-resizable": "^2.3.0",
    "vue-fragment": "^1.5.2",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.3",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vue/cli-plugin-babel": "^5.0.3",
    "@vue/cli-plugin-typescript": "^5.0.3",
    "@vue/cli-service": "^5.0.3",
    "@vue/eslint-config-prettier": "^5.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-playwright": "^0.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
